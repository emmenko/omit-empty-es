  "keywords": [
    "clear",
    "delete",
    "empty",
    "find",
    "has",
    "hasown",
    "javascript",
    "js",
    "key",
    "keys",
    "node",
    "node-js",
    "object",
    "omit",
    "properties",
    "property",
    "remove",
    "util",
    "utilities",
    "utility",
    "value"
